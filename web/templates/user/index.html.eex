<h1>Users</h1>

<table class="table">
  <%= for user <- @users do %>
  <tr>
    <td><strong><%= user.id %></strong></td>
    <td><strong><%= render "user.html", user: user %></strong></td>
    <td><strong><%= link "View", to: user_path(@conn, :show, user.id) %></strong></td>
  </tr>
  <% end %>
</table>
